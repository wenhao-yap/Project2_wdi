<h2>Dashboard</h2>
{{#if user}}
<button><a href="/tabs/new">Submit New Tab/Score</a></button>
{{/if}}
<table id="myTable">
    <thead>
    	<th>ID(song)</th>
    	<th>Song</th>
    	<th>Composer</th>
    	<th>Arranger</th>
    </thead>
    <tbody>
      {{#each songs}}
      <tr>
        <td>{{this.id}}</td>
        <td><a href="/tabs/{{this.id}}">{{this.name}}</a></td>
        <td>{{this.composer}}</td>
        <td>{{this.arranger}}</td>
        {{#if ../user}}
        <td>
        	<button><a href="/tabs/{{this.id}}">View</a></button>
        	<button><a href="/tabs/{{this.id}}/edit">Edit</a></button>
        	<form method="POST" action="/tabs/{{id}}?_method=DELETE" id="deleteButt">
        		<button type="submit">Delete</button>
        	</form>
        </td>
        {{/if}}
       </tr>
       {{/each}}
    </tbody> 
</table>
